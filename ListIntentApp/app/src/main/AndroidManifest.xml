<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kamaalhasan.listintentapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AustraliaList" />
        <activity android:name=".ChinaList" />
        <activity android:name=".CanadaList" />
        <activity android:name=".FranceList" />
        <activity android:name=".GermanyList" />
        <activity android:name=".HongkongList" />
        <activity android:name=".IndiaList" />
        <activity android:name=".IndonesiaList" />
        <activity android:name=".JapanList" />
        <activity android:name=".ItalyList"></activity>
    </application>

</manifest>